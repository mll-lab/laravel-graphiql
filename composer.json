{
  "name": "mll-lab/laravel-graphiql",
  "description": "Easily integrate GraphiQL into your Laravel project",
  "license": "MIT",
  "keywords": [
    "laravel",
    "graphiql",
    "graphql"
  ],
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^8",
    "illuminate/console": "^9 || ^10 || ^11",
    "illuminate/contracts": "^9 || ^10 || ^11",
    "illuminate/support": "^9 || ^10 || ^11"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.29",
    "larastan/larastan": "^2.5.2",
    "mll-lab/php-cs-fixer-config": "^5",
    "orchestra/testbench": "^7.7 || ^8.8 || ^9",
    "phpstan/extension-installer": "^1",
    "phpstan/phpstan": "^1.10.15",
    "phpstan/phpstan-mockery": "^1.1.1",
    "phpstan/phpstan-phpunit": "^1.3.11"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "MLL\\GraphiQL\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "kylekatarnls/update-helper": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\GraphiQL\\GraphiQLServiceProvider"
      ]
    }
  }
}
